        <h1 class="text-center text-secondary">Create Project</h1>



        {{!-- MIdlle contents --}}

        <br>
        <br>
        <form action="/create" method="POST" id="create">

            <div class="container justify-content-center">
                <hr>
                <hr>


                <div class="form-group">
                    <h4 class="font-weight-bold text-center text-secondary">Team Size</h4>
                    <input type="radio" id="male" name="devloper" value="Individual">
                    <label for="male">Individual</label>
                    <input class="ml-4" type="radio" id="female" name="devloper" value="Less than 4 Developer">
                    <label for="female">Less than 4 Developer</label>
                    <input class="ml-4" type="radio" id="female" name="devloper" value="More Than 4 Developer">
                    <label for="female">More Than 4 Developer</label><br>
                    <br>
                    <hr>
                    <hr>

                    <h4 class="font-weight-bold text-center text-secondary">Details</h4>
                    <label for="exampleFormControlInput1" class="text-secondary font-weight-bold ">Title
                    </label>
                    <input type="text" class="form-control" id="exampleFormControlInput1" placeholder="Enter Title"
                        name="title">
                    <br>
                    <br>

                    <div class="col-xs-3">

                        <label for="exampleFormControlInput1"
                            class="text-secondary font-weight-bold">Description</label>


                        <div class="modal-body">
                            <textarea class="form-control col-xs-12" name="description" form="create"
                                placeholder="Enter Description" rows="15" cols="50"></textarea>
                        </div>

                        <br>
                        <br>

                        <label for="exampleFormControlInput1" class="text-secondary font-weight-bold ">Youtube Link
                        </label>
                        <input type="text" class="form-control" id="exampleFormControlInput1"
                            placeholder="Enter Youtube link" name="youtubelink">

                        <br>
                        <br>
                        <label for="exampleFormControlInput1" class="text-secondary font-weight-bold ">Github Link
                        </label>
                        <input type="text" class="form-control" id="exampleFormControlInput1"
                            placeholder=" Paste Github Link" name="githublink">

                        <br>
                        <br>
                        <label for="exampleFormControlInput1" class="text-secondary font-weight-bold ">Deploy Link
                        </label>
                        <input type="text" class="form-control" id="exampleFormControlInput1"
                            placeholder=" Paste Deploy Link" name="deploylink">




                        <br>
                        <br>
                        <hr>
                        <hr>
                        <br>
                        <button type="submit" id="createbtn" class="btn btn-primary btn-lg btn-block">Create</button>
                    </div>
                </div>
            </div>
        </form>
